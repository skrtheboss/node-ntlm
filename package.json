{
    "name": "node-ntlm",
    "version": "0.0.0",
    "license": "MIT",
    "author": "Denis Frenademetz",
    "scripts": {
        "format": "nx format:write",
        "check-formatting": "nx format:check",
        "prepare": "husky install",
        "build": "nx run-many --target=build",
        "lint": "nx run-many --target=lint",
        "test": "nx run-many --target=build",
        "cleanup-lockfile": "yarn-deduplicate --strategy fewer",
        "check-lockfile": "yarn-deduplicate --strategy fewer -l -f",
        "publish": "nx run-many --target=publish",
        "replace-placeholder-versions": "node tools/replace-placeholders.mjs",
        "prepack": "nx run-many --target=build && yarn replace-placeholder-versions"
    },
    "private": true,
    "engines": {
        "node": ">=16.19.0"
    },
    "dependencies": {
        "des.js": "1.0.1",
        "js-md4": "0.3.2",
        "tslib": "2.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.4.2",
        "@commitlint/config-angular": "17.4.2",
        "@nrwl/devkit": "15.6.3",
        "@nrwl/eslint-plugin-nx": "15.6.3",
        "@nrwl/jest": "15.6.3",
        "@nrwl/js": "15.6.3",
        "@nrwl/linter": "15.6.3",
        "@nrwl/nx-cloud": "15.0.3",
        "@nrwl/workspace": "15.6.3",
        "@types/jest": "28.1.8",
        "@types/node": "16.18.11",
        "@typescript-eslint/eslint-plugin": "5.50.0",
        "@typescript-eslint/parser": "5.50.0",
        "eslint": "8.33.0",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-unicorn": "45.0.2",
        "husky": "8.0.3",
        "jest": "28.1.3",
        "jest-environment-jsdom": "28.1.3",
        "lerna": "6.4.1",
        "lint-staged": "13.1.0",
        "nx": "15.6.3",
        "prettier": "2.8.3",
        "pretty-quick": "3.1.3",
        "ts-jest": "28.0.8",
        "ts-node": "10.9.1",
        "typescript": "4.8.4",
        "yarn-deduplicate": "6.0.1"
    }
}
