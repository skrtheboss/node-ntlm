        "httpreq": "0.5.2",
        "js-md4": "0.3.2",
        "tslib": "2.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "17.4.4",
        "@commitlint/config-angular": "17.4.4",
        "@jscutlery/semver": "^2.30.0",
        "@nrwl/devkit": "15.6.3",
        "@nrwl/eslint-plugin-nx": "15.6.3",
        "@nrwl/jest": "15.6.3",
        "@nrwl/js": "15.6.3",
        "@nrwl/linter": "15.6.3",
        "@nrwl/nx-cloud": "15.0.3",
        "@nrwl/workspace": "15.6.3",
        "@types/jest": "28.1.8",
        "@types/node": "16.18.14",
        "@typescript-eslint/eslint-plugin": "5.54.0",
