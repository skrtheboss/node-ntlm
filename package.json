        "eslint": "8.36.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-unicorn": "45.0.2",
        "husky": "8.0.3",
        "jest": "28.1.3",
        "jest-environment-jsdom": "28.1.3",
        "lint-staged": "13.1.2",
        "nock": "^13.3.0",
        "nx": "15.6.3",
        "prettier": "2.8.7",
        "pretty-quick": "3.1.3",
